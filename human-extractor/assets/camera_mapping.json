{
  "_comment": "Dashcam File ID to Camera Mapping",
  "_description": "Maps file ID ranges to specific camera feeds",
  "_version": "1.0",

  "cameras": {
    "Movie_F": {
      "description": "Front camera during driving (Movie mode)",
      "suffix": "A",
      "id_range": {
        "min": 51000,
        "max": 52999
      },
      "examples": [
        "20250724151205_051924A.MP4",
        "20250725182330_052136A.MP4"
      ]
    },
    "Movie_R": {
      "description": "Rear camera during driving (Movie mode)",
      "suffix": "B",
      "id_range": {
        "min": 51000,
        "max": 52999
      },
      "examples": [
        "20250724151206_051925B.MP4",
        "20250725182331_052137B.MP4"
      ]
    },
    "Park_F": {
      "description": "Front camera during parking (Park mode)",
      "suffix": "A",
      "id_range": {
        "min": 53000,
        "max": 99999
      },
      "examples": [
        "20251006103805_070701A.MP4",
        "20250807020138_055552A.MP4"
      ]
    },
    "Park_R": {
      "description": "Rear camera during parking (Park mode)",
      "suffix": "B",
      "id_range": {
        "min": 53000,
        "max": 99999
      },
      "examples": [
        "20251006142644_070785B.MP4",
        "20250725062757_052085B.MP4"
      ]
    }
  },

  "detection_rules": [
    {
      "rule": "Suffix A + ID < 53000 → Movie_F",
      "priority": 1
    },
    {
      "rule": "Suffix B + ID < 53000 → Movie_R",
      "priority": 2
    },
    {
      "rule": "Suffix A + ID ≥ 53000 → Park_F",
      "priority": 3
    },
    {
      "rule": "Suffix B + ID ≥ 53000 → Park_R",
      "priority": 4
    }
  ],

  "source_directories": {
    "Park_R": "G:\\My Drive\\PROJECTS\\INVESTIGATION\\DASHCAM\\Park_R",
    "Park_F": "G:\\My Drive\\PROJECTS\\INVESTIGATION\\DASHCAM\\Park_F",
    "Movie_R": "G:\\My Drive\\PROJECTS\\INVESTIGATION\\DASHCAM\\Movie_R",
    "Movie_F": "G:\\My Drive\\PROJECTS\\INVESTIGATION\\DASHCAM\\Movie_F"
  }
}
